<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane minHeight="720.0" minWidth="720.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.AddressController">
	<children>
		<VBox alignment="CENTER" prefHeight="720.0" prefWidth="720.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<HBox alignment="CENTER" prefHeight="356.0" prefWidth="718.0" spacing="20.0">
					<children>
						<VBox maxWidth="300.0" minWidth="180.0" prefHeight="196.0" prefWidth="598.0" spacing="7.0">
							<children>
								<Label text="Address">
									<font>
										<Font size="15.0" />
									</font>
								</Label>
								<TextField fx:id="address1">
									<font>
										<Font size="15.0" />
									</font>
								</TextField>
								<Label text="District">
									<font>
										<Font size="15.0" />
									</font>
								</Label>
								<TextField fx:id="district">
									<font>
										<Font size="15.0" />
									</font>
								</TextField>
								<Label text="City">
									<font>
										<Font size="15.0" />
									</font>
								</Label>
								<ComboBox fx:id="city" prefWidth="150.0" />
                        <AnchorPane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Label layoutY="14.0" text="Coordinates">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </Label>
                              <Label layoutX="9.0" layoutY="36.0" text="X">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </Label>
                              <Label layoutX="160.0" layoutY="36.0" text="Y">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </Label>
                              <TextField layoutY="61.0" prefHeight="31.0" prefWidth="130.0" fx:id="xCoord">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </TextField>
                              <TextField fx:id="yCoord" layoutX="153.0" layoutY="61.0" prefHeight="31.0" prefWidth="130.0">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </TextField>
                           </children>
                        </AnchorPane>
							</children>
						</VBox>
						<VBox maxWidth="300.0" minWidth="180.0" prefHeight="196.0" prefWidth="598.0" spacing="7.0">
							<children>
								<Label text="Address 2">
									<font>
										<Font size="15.0" />
									</font>
								</Label>
								<TextField fx:id="address2">
									<font>
										<Font size="15.0" />
									</font>
								</TextField>
								<Label text="Phone">
									<font>
										<Font size="15.0" />
									</font>
								</Label>
								<TextField fx:id="phone">
									<font>
										<Font size="15.0" />
									</font>
								</TextField>
								<Label text="Postal Code">
									<font>
										<Font size="15.0" />
									</font>
								</Label>
								<TextField fx:id="postalCode">
									<font>
										<Font size="15.0" />
									</font>
								</TextField>
								<HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
									<children>
										<Button alignment="CENTER" mnemonicParsing="false" onAction="#add" text="Add" />
										<Button alignment="CENTER" mnemonicParsing="false" onAction="#update" text="Update" />
										<Button alignment="CENTER" mnemonicParsing="false" onAction="#delete" text="Delete" />
									</children>
								</HBox>
							</children>
							<HBox.margin>
								<Insets />
							</HBox.margin>
						</VBox>
					</children>
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</HBox>
				<TableView fx:id="tableView" VBox.vgrow="ALWAYS">
					<columns>
						<TableColumn minWidth="75.0" prefWidth="75.0" text="Address ID">
							<cellValueFactory>
								<PropertyValueFactory property="id" />
							</cellValueFactory>
						</TableColumn>

						<TableColumn minWidth="150.0" prefWidth="150.0" text="Address">
							<cellValueFactory>
								<PropertyValueFactory property="address" />
							</cellValueFactory>
						</TableColumn>

						<TableColumn minWidth="150.0" prefWidth="150.0" text="Address 2">
							<cellValueFactory>
								<PropertyValueFactory property="address2" />
							</cellValueFactory>
						</TableColumn>

						<TableColumn minWidth="100.0" prefWidth="100.0" text="District">
							<cellValueFactory>
								<PropertyValueFactory property="district" />
							</cellValueFactory>
						</TableColumn>

						<TableColumn minWidth="120.0" prefWidth="120.0" text="City">
							<cellValueFactory>
								<PropertyValueFactory property="city" />
							</cellValueFactory>
						</TableColumn>

						<TableColumn minWidth="100.0" prefWidth="100.0" text="Postal Code">
							<cellValueFactory>
								<PropertyValueFactory property="postalCode" />
							</cellValueFactory>
						</TableColumn>

						<TableColumn minWidth="100.0" prefWidth="100.0" text="Phone">
							<cellValueFactory>
								<PropertyValueFactory property="phone" />
							</cellValueFactory>
						</TableColumn>

						<TableColumn minWidth="150.0" prefWidth="150.0" text="Last Updated">
							<cellValueFactory>
								<PropertyValueFactory property="lastUpdate" />
							</cellValueFactory>
						</TableColumn>
					</columns>
					<columnResizePolicy>
						<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
					</columnResizePolicy>
				</TableView>
			</children>

		</VBox>
	</children>
</AnchorPane>
