    "langfile-versions": "node langfileVersions.js",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run prebuild && ng run storybook:storybook",
    "build-storybook": "npm run prebuild && ng run storybook:build-storybook",
    "generate:graphql": "graphql-codegen",
    "generate:graphql:watch": "graphql-codegen --watch"
  },
